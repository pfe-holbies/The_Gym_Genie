{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "description": "Client Side of GymGenie a MERN GraphQL Stack web app consuming the third party API of OpenAI to generate Text Completions for the user.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.js src/**/*.jsx",
    "test": "vitest --config vitest.config.cjs tests/**/*.test.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "bootstrap": "^5.2.3",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.1",
    "react-toastify": "^9.1.3",
    "vitest": "^0.31.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.8",
    "vite": "^4.3.2"
  }
}
